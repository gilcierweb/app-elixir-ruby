<% @result.each do |row| %>
  <h3>Detalhes <%= row['name'] %></h3>
  <div class="card">
    <img class="card-img-top" src=".../100px180/" alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title"><%= row['name'] %></h5>
      <p class="card-text"><%= row['full_name'] %>.</p>
      <p class="card-text">
        <small class="text-muted">Last updated 3 mins ago</small>
      </p>
    </div>
  </div>
  <br>
<% end %>